<div class="uk-margin uk-width-2xlarge uk-margin-auto uk-card uk-card-default uk-card-body uk-box-shadow-large">
    <table class="uk-table">
        <caption>
            POIs
        </caption>
        <thead>
        <th>Name</th>
        <th>Email</th>
        <th>Account Type</th>
        <th>Remove</th>
        </thead>
        <tbody class="uk-text-left">
        {{#each users}}
            <tr>
                <td> {{firstName}} {{lastName}} </td>
                <td> {{email}} </td>
                {{#if isAdmin}}
                    <td>Administrator</td>
                {{else}}
                    <td>Customer</td>
                {{/if}}
                <td><button class= "uk-button" >Delete</button> </td>
            </tr>
        {{/each}}
        </tbody>
    </table>
</div>